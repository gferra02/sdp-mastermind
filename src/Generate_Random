import java.util.Random


var num1:Int=0
var num2:Int=0
var num3:Int=0
var num4:Int=0

fun genRandomSequence():String {

    fun genRandomNum():Int {



        return Random().nextInt((5-0))
    }

    num1=genRandomNum()
    num2=genRandomNum()
    num3=genRandomNum()
    num4=genRandomNum()

    return   (num1.toString()+num2.toString()+num3.toString()+num4.toString())

}
